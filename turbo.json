{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULTS$", ".env", ".env.local", ".env.dev"],
            "outputs": [".nuxt/**", ".output/**", "dist/**"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "start": {
            "dependsOn": ["build"],
            "cache": false,
            "persistent": true
        }
    }
}
